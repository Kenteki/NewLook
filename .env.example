# Database - Use your Render PostgreSQL connection string
ConnectionStrings__ApplicationDbContext=User ID=postgres;Password=your_password;Host=your_host;Port=5432;Database=your_db;

# App Configuration - Set this to your Render app URL or localhost
APP_BASE_URL=https://your-app.onrender.com

# JWT Configuration - Generate a secure random key
Jwt__Key=your-secure-jwt-key-at-least-256-bits-long
Jwt__Issuer=YourIssuer
Jwt__Audience=YourAudience
Jwt__ExpiryInMinutes=2000

# Google OAuth - Get from Google Cloud Console
Authentication__Google__ClientID=your-google-client-id
Authentication__Google__ClientSecret=your-google-client-secret

# Github OAuth - Get from GitHub Developer Settings
Authentication__Github__ClientID=your-github-client-id
Authentication__Github__ClientSecret=your-github-client-secret

# SendGrid Email - Get from SendGrid Dashboard
SendGrid__ApiKey=your-sendgrid-api-key
SendGrid__FromEmail=your-email@domain.com
SendGrid__FromName=NewLook

# Cloudinary - Get from Cloudinary Dashboard
Cloudinary__CloudName=your-cloudinary-name
Cloudinary__ApiKey=your-cloudinary-key
Cloudinary__ApiSecret=your-cloudinary-secret

# Salesforce - Get from Salesforce Developer Org
Salesforce__InstanceUrl=https://yourinstance.salesforce.com
Salesforce__ClientId=your-salesforce-client-id
Salesforce__ClientSecret=your-salesforce-client-secret
Salesforce__Username=your-salesforce-username
Salesforce__Password=your-salesforce-password
Salesforce__SecurityToken=your-salesforce-security-token
Salesforce__ApiVersion=v59.0
Salesforce__TokenEndpoint=https://login.salesforce.com/services/oauth2/token
